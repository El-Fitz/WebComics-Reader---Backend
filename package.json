{
  "name": "WebComics Reader - Backend",
  "version": "0.0.1",
  "description": "WebComics Reader - Backend",
  "scripts": {
    "test": "jest --coverage"
  },
  "jest": {
    "testURL": "http://localhost/",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "__tests__/.*\\.test\\.(ts|tsx|js)$"
  },
  "dependencies": {
    "vandium": "^4.8.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "jest": "^23.6.0",
    "mocha": "^5.2.0",
    "serverless-domain-manager": "^2.6.5"
  }
}
